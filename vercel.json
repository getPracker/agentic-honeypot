{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "api/ping.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/ping",
            "dest": "api/ping.py"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ]
}