{
    "version": 2,
    "functions": {
        "api/index.py": {
            "maxDuration": 30,
            "memory": 1024
        },
        "api/ping.py": {
            "maxDuration": 10,
            "memory": 512
        }
    },
    "routes": [
        {
            "src": "/ping",
            "dest": "api/ping.py"
        },
        {
            "src": "/health",
            "dest": "api/index.py"
        },
        {
            "src": "/api/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ]
}